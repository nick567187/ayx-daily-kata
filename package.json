  },
  "homepage": "https://github.com/nick567187/ayx-daily-kata#readme",
  "dependencies": {
    "@types/jest": "^26.0.10",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  }
}
