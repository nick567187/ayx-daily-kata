  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": ["<rootDir>/src"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/vendor/**"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testEnvironment": "node",
    "rootDir": "."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nick567187/ayx-daily-kata.git"
